<base:BaseWindow
	x:Class="RestClientLibrary.Screen.Windows.ViewAppSettingsWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:base="clr-namespace:DataLibrary;assembly=DataLibrary"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:RestClientLibrary.Screen.Windows"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:screen="clr-namespace:RestClientLibrary.Screen"
	Title="Application Settings"
	MinHeight="200"
	MinWidth="350"
	Background="{DynamicResource AlternateBackgroundBrush}"
	Foreground="{DynamicResource PrimaryForeBrush}"
	ResizeMode="NoResize"
	SizeToContent="WidthAndHeight"
	WindowStartupLocation="CenterOwner"
	WindowStyle="ToolWindow"
	mc:Ignorable="d">
	<Grid>
		<screen:ucSettings x:Name="ucSettings" />
	</Grid>
</base:BaseWindow>
